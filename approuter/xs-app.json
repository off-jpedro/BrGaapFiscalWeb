{
    "welcomeFile": "/index.html",
    "authenticationMethod": "none",
    "routes": [
      {
        "source": "^/resources/(.*)$",
        "target": "$1",
        "service": "html5-apps-repo-rt",
        "authenticationType": "none"
      },
      {
        "source": "^/api/(.*)$",
        "target": "$1",
        "destination": "srv-api",
        "authenticationType": "none"
      },
      {
        "source": "^(.*)$",
        "target": "$1",
        "localDir": "resources/webapp"
      }
    ]
  }