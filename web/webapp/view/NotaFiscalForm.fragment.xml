            <VBox id="notaFiscalDialogVBox">
                <Text id="clienteNomeText" text="Nome do Cliente:" />
                <Text id="clienteNomeValue" text="{notaFiscal>cliente/nome}" />
                <Text id="fornecedorNomeText" text="Nome do Fornecedor:" />
                <Text id="fornecedorNomeValue" text="{notaFiscal>fornecedor/nome}" />
                <Text id="numeroNotaText" text="NÃºmero da Nota:" />
                <Text id="numeroNotaValue" text="{notaFiscal>numeroNota}" />
                <Text id="valorNotaText" text="Valor da Nota:" />
                <Text id="valorNotaValue" text="{notaFiscal>valorNota}" />
            </VBox>
        </content>
        <beginButton>
            <Button id="cancelButton" text="Cancelar" press="onCloseNotaFiscalForm" />
        </beginButton>
    </Dialog>
</core:FragmentDefinition>